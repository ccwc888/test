    "@input-placeholder-color": "#cccccc",
    "@input-color": "#333333",
    "@input-border-color": "#e5e5e5",
    "@input-hover-border-color": "#999999",
    "@input-height-base": "32px",
    "@input-padding-horizontal": "9px",
    "@input-padding-vertical-base": "4px",
    "@input-disabled-bg": "#f5f5f5",
